{
  "name": "offstar-ai-dev-platform",
  "version": "1.0.0",
  "description": "OFFSTAR AI Development Platform - AI-powered prototyping with integrated services",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "backend": "ts-node backend/server.ts",
    "ai-services": "python ai-services/main.py",
    "setup": "npm install && pip install -r requirements.txt",
    "prototype:create": "node scripts/create-prototype.js",
    "prototype:ai-generate": "node scripts/ai-generate.js",
    "prototype:deploy": "npm run build && npm run deploy:replit",
    "plan:feature": "python scripts/obl-planner.py",
    "process:ai-task": "python scripts/ionet-processor.py",
    "deploy:replit": "npm run build && replit deploy",
    "deploy:vercel": "npm run build && vercel --prod",
    "deploy:production": "npm run build && npm run deploy:vercel",
    "mobile:connect": "node scripts/mobile-connect.js",
    "quantum:sync": "python scripts/quantum-sync.py",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "codegen": "node scripts/ai-codegen.js",
    "optimize": "python scripts/optimize.py"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "framer-motion": "^10.0.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "@tanstack/react-query": "^5.0.0",
    "recharts": "^2.8.0",
    "lucide-react": "^0.294.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "axios": "^1.6.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "ws": "^8.14.0",
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "zustand": "^4.4.0",
    "react-hot-toast": "^2.4.0",
    "monaco-editor": "^0.44.0",
    "@monaco-editor/react": "^4.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/ws": "^8.5.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.0",
    "jest": "^29.0.0",
    "@testing-library/react": "^13.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "concurrently": "^8.2.0"
  },
  "keywords": [
    "ai",
    "development-platform",
    "ionet",
    "replit",
    "codespaces",
    "obl.dev",
    "prototyping",
    "automation",
    "quantum",
    "consciousness"
  ],
  "author": "OFFSTAR Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/protechtimenow/offstar-ai-dev-platform.git"
  }
}